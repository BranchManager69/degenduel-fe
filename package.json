  "scripts": {
    "----------- TYPE CHECKING COMMANDS --------------": "",
    "type-check": "tsc --noEmit",
    "---------- PREFERRED BUILD COMMANDS -------------": "",
    "build:prod": "tsc --noEmit && node --max_old_space_size=8192 ./node_modules/.bin/vite build --mode production",
    "build:dev": "tsc --noEmit && VITE_FORCE_DISABLE_MINIFY=true node --max_old_space_size=8192 ./node_modules/.bin/vite build --mode development",
    "build:local": "bash ./scripts/build-and-run-local.sh",
    "---------- ALTERNATE BUILD COMMANDS -------------": "",
    "hmr": "vite --port 3006",
    "build": "npm run build:prod",
    "build:quick": "vite build --mode quick",
    "build:analyze": "node scripts/analyze-build.js",
    "preview": "vite preview",
    "dev:local": "vite --mode dev-local",
    "--------------- DESIGN COMMANDS -----------------": "",
    "design": "npm run storybook",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "--------------- TESTING COMMANDS ----------------": "",
    "test": "jest --config jest.config.ts --verbose",
    "--------------- LINTING COMMANDS ----------------": "",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --config .eslintrc.json src",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --config .eslintrc.json src --fix",
    "--------------- AI REVIEW COMMANDS --------------": "",
    "OLD-lint:ai": "claude -p 'you are a linter. please look at the changes vs. main and report what changes you see. report the filename and line number on one line, and a description of the issue on the second line. do not return any other text.'",
    "OLD-lint:ai:commit": "claude -p 'you are a linter. please look at the changes that are yet to be committed and report what changes you see. report the filename and line number on one line, and a description of the change on the second line. do not return any other text.'",
    "--------------- OTHER COMMANDS ------------------": ""
  },
