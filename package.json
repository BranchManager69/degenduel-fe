{
  "name": "degenduel-fe",
  "description": "DegenDuel",
  "author": "Branch Manager<branch@branch.bet>",
  "private": true,
  "version": "1.9.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "----------- TYPE CHECKING COMMANDS --------------": "",
    "type-check": "tsc --noEmit",
    "type-check-direct": "tsc --noEmit",
    "---------- PREFERRED BUILD COMMANDS -------------": "",
    "build:prod": "tsc --noEmit && npx --node-arg=--max_old_space_size=8192 vite build --mode production",
    "build:dev": "tsc --noEmit && VITE_FORCE_DISABLE_MINIFY=true npx --node-arg=--max_old_space_size=8192 vite build --mode development",
    "build:local": "bash ./scripts/build-and-run-local.sh",
    "---------- ALTERNATE BUILD COMMANDS -------------": "",
    "hmr": "npx vite --port 3006",
    "build": "npm run build:prod",
    "build:quick": "npx vite build --mode quick",
    "build:prod:analyze": "VITE_FORCE_DISABLE_MINIFY=false npx --node-arg=--max_old_space_size=8192 vite build --mode production && npx source-map-explorer 'dist/assets/*.js' --html dist/bundle-analysis.html --tsv dist/bundle-analysis.tsv && echo '\nBundle analysis report available at: https://degenduel.me/bundle-analysis.html\nDetailed TSV data available at: https://degenduel.me/bundle-analysis.tsv\n'",
    "build:dev:analyze": "npx --node-arg=--max_old_space_size=8192 vite build --mode development && npx source-map-explorer 'dist-dev/assets/*.js' --html dist-dev/bundle-analysis.html --tsv dist-dev/bundle-analysis.tsv > dist-dev/bundle-analysis.log 2>&1 && echo '\nDev bundle analysis report available at: https://dev.degenduel.me/bundle-analysis.html\nDetailed dev TSV data available at: https://dev.degenduel.me/bundle-analysis.tsv\nFull analysis log saved to: dist-dev/bundle-analysis.log\n'",
    "preview": "npx vite preview",
    "dev:local": "npx vite --mode dev-local",
    "--------------- DESIGN COMMANDS -----------------": "",
    "design": "npm run storybook",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "--------------- TESTING COMMANDS ----------------": "",
    "test": "jest --config jest.config.ts --verbose",
    "--------------- LINTING COMMANDS ----------------": "",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --config .eslintrc.json src",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --config .eslintrc.json src --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md}\"",
    "prepare": "husky",
    "--------------- ADMIN TOOLS ---------------------": "",
    "new:page": "node scripts/generate-admin-page.js",
    "--------------- AI REVIEW COMMANDS --------------": "",
    "OLD-lint:ai": "claude -p 'you are a linter. please look at the changes vs. main and report what changes you see. report the filename and line number on one line, and a description of the issue on the second line. do not return any other text.'",
    "OLD-lint:ai:commit": "claude -p 'you are a linter. please look at the changes that are yet to be committed and report what changes you see. report the filename and line number on one line, and a description of the change on the second line. do not return any other text.'",
    "--------------- OTHER COMMANDS ------------------": ""
  },
